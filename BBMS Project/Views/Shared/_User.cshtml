                                    <i class="fa fa-envelope-o fa-contact"></i> <p><a href="mailto:support@donation.com<">support@donation.com</a><br /><a href="mailto:helpme@donation.com">helpme@donation.com</a></p>

                                    <i class="fa fa-location-arrow fa-contact"></i> <p> Amnex Blood Management System, <br />SG Highway, Ahmedabad</p>

                                    <i class="fa fa-phone fa-contact"></i> <p>Office:&nbsp; +91 9876543210<br />Cell:&nbsp; +91 9638527410</p>

                                </div>

                            </div> <!-- end .footer-widget-wrapper  -->

                        </div> <!--  end .footer-widget  -->

                    </div> <!--  end .col-md-4 col-sm-12 -->

                    <div class="col-md-4 col-sm-12 col-xs-12">

                        <div class="footer-widget clearfix">

                            <div class="sidebar-widget-wrapper">

                                <div class="footer-widget-header clearfix">
                                    <h3> Links</h3>
                                </div>  <!--  end .footer-widget-header -->


                                <ul class="footer-useful-links">

                                    <li>
                                        <a href="@Url.Action("Homepage", "Home")">
                                            <i class="fa fa-caret-right fa-footer"></i>
                                            <span>Home</span>
                                        </a>
                                    </li>



                                    <li>
                                        <a href="@Url.Action("GetAllBloodBankDetails", "BloodBank")">

                                            <i class="fa fa-caret-right fa-footer"></i>
                                            <span>Blood Bank Details </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("About", "Home")">
                                            <i class="fa fa-caret-right fa-footer"></i>
                                            <span>About Us </span>
                                        </a>
                                    </li>

                                    <li>
                                        <a href="@Url.Action("AboutBloodDonate", "Admin")">
                                            <i class="fa fa-caret-right fa-footer"></i>
                                            <span>Blood Donation </span>
                                        </a>
                                    </li>

                                    <li>
                                        <a href="@Url.Action("SeekerLogin", "Seeker")">
