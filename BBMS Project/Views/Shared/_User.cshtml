<body>

    <div id="preloader">
        <span class="margin-bottom">    </span>
    </div>

    <!--  HEADER -->

    <header class="main-header clearfix" data-sticky_header="true">

        <div class="top-bar clearfix">

            <div class="container">

                <div class="row">
                     
                    <div class="col-md-8 col-sm-12">

                        <p>
                            @*<span>  <strong>Contact: </strong> Amnex Infotechnologies, Ahmedabad</span>*@
                            <span>  <i class="fa fa-phone"></i> <strong>Call Us :</strong> +91 9876543210</span>
                        </p>

                    </div>

                    <div class="col-md-4col-sm-12">
                        <div class="top-bar-social">
                            <a href="https://www.facebook.com/">
                                <i class="fa fa-facebook rounded-box"></i>
                            </a>
                            <a href=""><i class="fa fa-twitter rounded-box"></i></a>
                            <a href="https://twitter.com/login"><i class="fa fa-google-plus rounded-box"></i></a>
                            <a href="https://www.instagram.com/"><i class="fa fa-instagram rounded-box"></i></a>
                            <a href="https://www.youtube.com/"><i class="fa fa-youtube rounded-box"></i></a>
                        </div>
                    </div>

                </div>

            </div> <!--  end .container -->

        </div> <!--  end .top-bar  -->

        <section class="header-wrapper navgiation-wrapper">

            <div class="navbar navbar-default">
                <div class="container">

                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="logo" href="@Url.Action("HomePage", "Home")"><img alt="" src="~/UserUI/images/logo.png"></a>
                    </div>

                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav navbar-right">

                            <li>
                                <a href='@Url.Action("HomePage", "Home")'>Home </a>
                            </li>

                            <li>

                                <a href='@Url.Action("About", "Home")'>About us </a>
                            </li>

                            <li class="drop">
                                <a href="#">Looking For Blood</a>
                                <ul class="drop-down">

                                   @* <li>
                                        <a href="@Url.Action("GetAllBloodStock", "BloodStock")">Blood Availability</a>
                                    </li>*@

                                    <li>
                                        <a href="@Url.Action("getBloodBankDetails", "BloodBank")">Blood Bank Directory</a>
                                    </li>

                                    <li>
                                        @if (Session["SeekerID"] == null)
                                        {
                                            <a href='@Url.Action("SeekerLogin", "Seeker")'> Seeker Login </a>
                                        }

                                    </li>
                                </ul>
                            </li>

                            <li>
                                <a href="#"> Donate Blood</a>
                                <ul class="drop-down">
                                    <li><a href="@Url.Action("getCampDetails", "Camp")">Blood Donation Camp</a></li>
                                    <li><a href="@Url.Action("DonorLogin", "Donor")">Donor Login</a></li>
                                    <li><a href="@Url.Action("AboutBloodDonate", "Admin")">About Blood Donation</a></li>
                                </ul>
                            </li>

                            <li>
                                <a href="#">Blood Bank Login</a>
                                <ul class="drop-down">
                                    <li><a href="@Url.Action("bloodBankLogin", "BloodBank")">Blood Bank Login</a></li>
                                    <li><a href="@Url.Action("AddBloodBank", "BloodBank")">Add Your Blood Bank</a></li>
                                </ul>
                            </li>

                            <li>
                                <a href="@Url.Action("Contact", "Home")"> Contact us </a>

                            </li>
                        </ul>
                    </div>
                </div>
            </div>

        </section>

    </header> <!-- end main-header  -->

    <div>
        @RenderBody()
