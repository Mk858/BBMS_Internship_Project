                                            <span>Blood Bank Login</span>

                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Contact", "Home")">
                                            <i class="fa fa-caret-right fa-footer"></i>
                                            <span>Contact Us </span>
                                        </a>
                                    </li>



