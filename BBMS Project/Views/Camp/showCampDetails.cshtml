@model IEnumerable<BBMS_Project.Models.Camp>

@{
    Layout = "~/Views/Shared/_UTemp.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.state)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.city)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contactno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.conductedby)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.organizedby)
        </th>
       @* <th>
            @Html.DisplayNameFor(model => model.schedstart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.schedend)
        </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.state)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.city)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contactno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.conductedby)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.organizedby)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.schedstart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.schedend)
            </td>*@
        </tr>
    }

</table>
